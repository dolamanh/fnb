{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Android",
      "type": "reactnative",
      "request": "launch",
      "platform": "android",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Debug iOS", 
      "type": "reactnative",
      "request": "launch",
      "platform": "ios",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Attach to packager",
      "type": "reactnative",
      "request": "attach",
      "port": 8081,
      "cwd": "${workspaceFolder}"
    }
  ]
}